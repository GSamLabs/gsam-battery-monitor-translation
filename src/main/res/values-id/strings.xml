    <!-- Only Applicable to Paid/Pro Version (or marketing of said version) -->
    <string name="appdetail_details_adjust_screen_market_bsd_title">Apakah Layar Anda Terlalu Cerah?</string>
    <string name="appdetail_details_adjust_screen_market_bsd_msg">Jika Anda memiliki kesulitan membaca di malam hari karena layar terlalu terang, periksa GSAM Layar dimmer. Ini memungkinkan Anda mengurangi kecerahan layar dibawah yang biasanya diperbolehkan, membuat untuk jauh lebih memudahkan membaca dimalam hari!\n\nPemberitahuan ini hanya sekali.</string>
    <string name="preferences_send_anon_stats_buypro_title">Beli Pro!</string>
    <string name="preferences_send_anon_stats_buypro_msg">Versi gratis dari Battery monitor tidak akan membiarkan Anda mematikan iklan jika Anda memilih untuk tidak mengizinkan statistik penggunaan data.\n\nVersi Pro tidak termasuk iklan, dan akan membiarkan Anda mematikan statistik penggunaan data.</string>
    <string name="menu_seeotherapps">Lihat aplikasi lain oleh GSam Labs</string>
    <string name="menu_gopro">Upgrade Ke Pro</string>
    <string name="menu_gopro_nook">Pembelian</string>
    <string name="menu_goprokindle">Upgrade Ke Edisi The Kindle Fire</string>
    <string name="nook_gopro_title">Trial Kadaluarsa</string>
    <string name="nook_gopro_msg">Versi Percobaan Anda dari aplikasi ini telah berakhir. Silahkan membeli edisi penuh. Anda dapat terus menggunakan monitor baterai, namun akan berjalan dalam mode Gratis yang menghilangkan sejumlah fitur dan optimasi.</string>
    <string name="nook_gopro_ok">Beli Versi Pro</string>
    <string name="nook_gopro_cancel">Melanjutkan Versi Gratis</string>
    <string name="main_market_pro_title">Terima Kasih Telah Menggunakan GSam Battery Monitor - Versi Gratis</string>
    <string name="main_market_pro_msg">Apakah Anda tahu ada versi optimalisasi Tablet GSAM Baterai Monitor? Unduh dari Play Store.\n\nPemberitahuan ini hanya sekali.</string>
    <string name="main_market_pro_msg_kindle">Apakah Anda tahu ada versi optimalisasi Kindle Fire GSAM Baterai Monitor?  Unduh dari Play Store.\n\nPemberitahuan ini hanya sekali.</string>
    <string name="main_market_pro_ok">Pratinjau</string>
    <string name="main_market_pro_cancel">Tidak Terima Kasih</string>
    <string name="main_invalid_license_title">Lisensi Tidak Sah</string>
    <string name="main_invalid_license_msg">Lisensi untuk aplikasi ini tampaknya tidak berlaku. The Battery Monitor akan terus beroperasi di dalamnya dalam mode Gratis. Jika Anda merasa ini adalah kesalahan, silakan cek pembelian Anda.</string>
