    <string name="profedit_cancel">Abbrechen</string>
    <string name="profedit_custom_values_saves">Gespeicherte eigene Werte. Du kannst die Herstellereinstellungen im Einstellungen Bildschirm wiederherstellen.</string>
    <string name="preferences_clear_chart_history_title">Leeren der Verlaufsdatei</string>
